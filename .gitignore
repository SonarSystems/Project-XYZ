.DS_Store
echo ".DS_Store" >> ~/.gitignore_global
echo "._.DS_Store" >> ~/.gitignore_global
echo "**/.DS_Store" >> ~/.gitignore_global
echo "**/._.DS_Store" >> ~/.gitignore_global
echo "*.ilk" >> ~/.gitignore_global
echo "._*.ilk" >> ~/.gitignore_global
echo "**/*.ilk" >> ~/.gitignore_global
echo "**/._*.ilk" >> ~/.gitignore_global
**/.vs
git config --global core.excludesfile ~/.gitignore_global
